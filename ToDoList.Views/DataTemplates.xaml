<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:ToDoList.Views"
                    xmlns:viewmodels="clr-namespace:ToDoList.ViewModels;assembly=ToDoList.ViewModels">

    <DataTemplate DataType="{x:Type viewmodels:ToDoTasksViewModel}">
        <views:ToDoTasksView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:AddToDoTaskViewModel}">
        <views:AddToDoTaskView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:EditToDoTaskViewModel}">
        <views:EditToDoTaskView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:UsersViewModel}">
        <views:UsersView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:AddUserViewModel}">
        <views:AddUserView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:EditUserViewModel}">
        <views:EditUserView />
    </DataTemplate>
</ResourceDictionary>